# Tanım: X11 Uzatma kablosu protokolünü düzeltir
# URL: https://xorg.freedesktop.org/
# Paketçi: Cihan_Alkan
# Gerekler: xproto xextproto xorg-util-macros
# Grup: 

isim=fixesproto
surum=5.0
devir=1
kaynak=(https://www.x.org/releases/individual/proto/fixesproto-5.0.tar.gz)

derle() {

  cd $isim-5.0

  ./configure --prefix=/usr
  make
  make DESTDIR="${PKG}" install
  install -Dm644 COPYING "${PKG}/usr/share/licenses/${isim}/COPYING"
}
