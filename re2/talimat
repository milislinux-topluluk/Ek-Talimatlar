# Gerekler: 

isim=re2
surum=2018-01-01
devir=1

kaynak=(https://github.com/google/re2/archive/$surum.tar.gz::$isim-$surum.tar.gz)

derle() {
cd $isim-$surum
  make
  make prefix=/usr DESTDIR="$PKG" install
  install -Dm644 LICENSE "$PKG/usr/share/licenses/$isim/LICENSE"
}
