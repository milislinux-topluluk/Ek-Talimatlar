# Tanım: Sudoku üreten ve çözen yazılım
# URL: https://qqwing.com/
# Paketçi: Cihan_Alkan
# Gerekler:
# Grup:

isim=qqwing
surum=1.3.4
devir=1

kaynak=(https://qqwing.com/$isim-$surum.tar.gz)

derle() {
cd $isim-$surum
    ./configure --prefix=/usr
  sed -i -e 's/ -shared / -Wl,-O1,--as-needed\0/g' libtool
  make
  make DESTDIR=$PKG install
}
