isim=python-pyrex
surum=0.9.9
devir=1
kaynak=(http://www.cosc.canterbury.ac.nz/~greg/python/Pyrex/Pyrex-${surum}.tar.gz)

derle() {
  cd Pyrex-${surum}
  python setup.py build

  python setup.py install -O1 --root="${PKG}" --prefix=/usr
}
