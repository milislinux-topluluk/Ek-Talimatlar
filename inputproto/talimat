# Tanım: X11 Giriş uzatma kablosu protokolü
# URL: https://userbase.kde.org/Apper
# Paketçi: Cihan_Alkan
# Gerekler: xorg-util-macros asciidoc
# Grup: 


isim=inputproto
surum=2.3.2
devir=1
kaynak=(https://xorg.freedesktop.org/releases/individual/proto/${isim}-${surum}.tar.bz2)

derle() {

  cd ${isim}-${surum}
  ./configure --prefix=/usr
  make

  make DESTDIR="${PKG}" install

  install -m755 -d "${PKG}/usr/share/licenses/${isim}"
  install -m644 COPYING "${PKG}/usr/share/licenses/${isim}/"
}
