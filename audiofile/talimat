# Tanım: Silicon Graphics Ses Dosyası Kütüphanesi
# URL: https://audiofile.68k.org/
# Paketçi: Cihan Alkan
# Gerekler: alsa-lib flac

isim=audiofile
surum=0.3.6
devir=1
kaynak=(https://audiofile.68k.org/$isim-$surum.tar.gz)

derle() {
  cd "$SRC/$isim-$surum"

  ./configure --prefix=/usr
  make

  make DESTDIR="$PKG" install
}
