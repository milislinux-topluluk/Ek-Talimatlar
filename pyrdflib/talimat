# Tanım: Bilgiyi temsil etmek için basit ama güçlü bir dil olan RDF ile çalışmak için bir Python kütüphanesi
# URL: http://rdflib.net/
# Paketçi: Cihan_Alkan
# Gerekler: python python3 python-setuptools python3-setuptools pymarkupsafe
# Grup: kütüphane

isim=pyrdflib
surum=4.2.2
devir=1
kaynak=(https://github.com/RDFLib/rdflib/archive/4.2.2.tar.gz::$isim-$surum.tar.gz)

derle() {

  cp -a rdflib-$surum{,-py3}

  cd "$SRC"/rdflib-$surum
  python setup.py build
  python setup.py install --root="$PKG" --optimize=1
 
  cd "$SRC"/rdflib-$surum-py3
  python3 setup.py build

  python3 setup.py install --root="$PKG" --optimize=1

}
