# Gerekler:
# Grup: kütüphane

isim=etl
surum=1.2.1
devir=1

kaynak=(https://downloads.sourceforge.net/project/synfig/releases/$surum/source/ETL-$surum.tar.gz)

derle() {

  cd ETL-$surum
  ./configure --prefix=/usr
  make
  make DESTDIR="$PKG" install
}
